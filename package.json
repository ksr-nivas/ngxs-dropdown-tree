{
  "name": "ngxs-treeview",
  "version": "6.0.2",
  "license": "MIT",
  "description": "An Angular treeview component with checkbox",
  "scripts": {
    "start": "ng serve",
    "lint": "ng lint",
    "test": "ng test",
    "test-coverage": "ng test --watch=false --code-coverage",
    "e2e": "ng e2e",
    "prebuild": "rimraf tmp dist",
    "build": "gulp inline-templates && ngc -p tsconfig-lib.json && copyfiles package.json README.md LICENSE dist",
    "postbuild": "rimraf tmp",
    "pub": "npm run build && npm publish dist",
    "predeploy": "rimraf dist",
    "deploy": "ng build --prod --base-href /ngxs-treeview/ && node gh-pages.js"
  },
  "keywords": [
    "ng",
    "ngxs",
    "angular",
    "ngxs-treeview",
    "tree",
    "treeview",
    "tree-view",
    "checkbox",
    "dropdown",
    "recursive",
    "angular tree component"
  ],
  "author": "Leo Vo <leovo2708@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leovo2708/ngxs-treeview.git"
  },
  "bugs": {
    "url": "https://github.com/leovo2708/ngxs-treeview/issues"
  },
  "main": "src/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "types": "src/index.d.ts",
  "private": false,
  "peerDependencies": {
    "@angular/common": ">=8.2.14",
    "@angular/core": ">=8.2.14",
    "@angular/forms": ">=8.2.14",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.21",
    "@angular/cli": "~8.3.21",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.2.15",
    "@angular/language-service": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@fortawesome/fontawesome": "^1.1.8",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.1.4",
    "bootstrap": "^4.4.1",
    "codecov": "^3.6.1",
    "codelyzer": "^5.2.1",
    "copyfiles": "^2.1.1",
    "core-js": "^3.6.1",
    "font-awesome": "^4.7.0",
    "gh-pages": "^2.1.1",
    "gulp": "^4.0.2",
    "gulp-inline-ng2-template": "^5.0.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.0.0",
    "karma-jasmine-html-reporter": "^1.5.1",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.0",
    "protractor": "^5.4.2",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "ts-node": "~8.5.4",
