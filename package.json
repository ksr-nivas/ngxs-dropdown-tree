{
  "name": "ngxs-dropdown-tree",
  "version": "6.0.4",
  "license": "MIT",
  "description": "An Angular treeview component with checkbox",
  "scripts": {
    "start": "ng serve",
    "lint": "ng lint",
    "test": "ng test",
    "test-coverage": "ng test --watch=false --code-coverage",
    "e2e": "ng e2e",
    "prebuild": "rimraf tmp dist",
    "build": "gulp inline-templates && ngc -p tsconfig-lib.json && copyfiles package.json README.md LICENSE dist",
    "postbuild": "rimraf tmp",
    "pub": "npm run build && npm publish dist",
    "predeploy": "rimraf dist",
    "deploy": "ng build --prod --base-href /ngxs-dropdown-tree/ && node gh-pages.js"
  },
  "keywords": [
    "ng",
    "ngxs",
    "angular",
    "ngxs-dropdown-tree",
    "tree",
    "treeview",
    "tree-view",
    "checkbox",
    "dropdown",
    "recursive",
    "angular tree component"
  ],
  "author": "Forked from: Leo Vo <leovo2708@gmail.com>; Modified by: Srini <sri.ui.consultant@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ksr-nivas/ngxs-dropdown-tree.git"
  },
  "homepage": "https://github.com/ksr-nivas/ngxs-dropdown-tree/blob/master/README.md",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "types": "src/index.d.ts",
  "private": false,
  "peerDependencies": {
    "@angular/common": "^9.1.11",
